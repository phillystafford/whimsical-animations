{
  "editor.defaultFormatter": "biomejs.biome",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.biome": "explicit"
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#fe65bb",
    "activityBar.background": "#fe65bb",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#8ffd02",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#fe65bb",
    "statusBar.background": "#fe32a4",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#fe65bb",
    "statusBarItem.remoteBackground": "#fe32a4",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "tab.activeBorder": "#fe65bb",
    "titleBar.activeBackground": "#fe32a4",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#fe32a499",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#fe32a4",
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  }
}
